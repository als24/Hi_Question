@charset "UTF-8";
@font-face {
  font-family: "iconfont";
  font-weight: normal;
  font-style: normal;
  src: url("data:application/x-font-woff2;charset=utf-8;base64,") format("woff2");
}
.hi-icon {
  display: inline-block;
  font-family: "iconfont" !important;
  font-size: 32rpx;
  font-weight: normal;
  font-style: normal;
  font-variant: normal;
  line-height: 1;
  letter-spacing: 0;
  text-transform: none;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.hi-icon.xss {
  font-size: 16rpx;
}
.hi-icon.xs {
  font-size: 20rpx;
}
.hi-icon.sm {
  font-size: 24rpx;
}
.hi-icon.md {
  font-size: 32rpx;
}
.hi-icon.lg {
  font-size: 42rpx;
}
.hi-icon:before {
  position: relative;
  z-index: 2;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.hi-icon-arrow-up:before {
  content: "";
}
.hi-icon-arrow-down:before {
  content: "";
}
.hi-icon-iconfontjiantouzuo:before {
  content: "";
}
