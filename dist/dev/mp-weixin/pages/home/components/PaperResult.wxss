.history-wrap {
  margin-top: 60rpx;
  font-size: 44rpx;
  font-weight: 600;
}
.history-wrap__title {
  margin-bottom: 30rpx;
  font-weight: 800;
}
.history-wrap__empty {
  width: 50%;
}
.history-wrap__empty--text {
  margin-top: 20rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #AAAAAA;
}
.history-wrap .paperlist {
  display: grid;
  gap: 20rpx 40rpx;
  grid-template-columns: 1fr 1fr;
}
.history-wrap .paperlist .paperitem {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 32rpx;
  font-weight: 400;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.history-wrap .paperlist .paperitem .title {
  font-weight: 600;
}
